{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 300,
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/login",
      "dest": "api/index.py"
    },
    {
      "src": "/manage",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ]
}
